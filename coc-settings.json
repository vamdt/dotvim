{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
	      "usePlaceholders": true
      }
    }, "rust": {
	    "command":"rust-analyzer",
	    "filetypes":["rust"],
      "rootPatterns": ["Cargo.toml"]
    }
  },
  "workspace.workspaceFolderCheckCwd": true
}
